{
    "name": "@mparticle/snippets",
    "version": "1.0.0",
    "author": "mParticle Developers <developers@mparticle.com> (https://www.mparticle.com)",
    "description": "mParticle",
    "files": [
        "dist/exportedFileFromRollups.js"
    ],
    "main": "dist/exportedFileFromRollups.js",
    "repository": "https://github.com/mparticle/TBD",
    "scripts": {
        "watch": "ENVIRONMENT=prod rollup --config rollup.config.js -w",
        "build": "npm run build:prod",
        "build:dev": "ENVIRONMENT=dev rollup --config rollup.config.js",
        "build:prod": "ENVIRONMENT=prod rollup --config rollup.config.js",
        "test": "npm run test:mocha",
        "test:mocha": "mocha -r ts-node/register test/**/*.test.ts",
        "test:mocha:junit": "mocha -r ts-node/register test/**/*.test.ts --reporter mocha-junit-reporter --reporter-options mochaFile=./test/reports/unit-tests/test-media-sdk.xml",
        "test:mocha:watch": "mocha -r ts-node/register --watch-extensions ts --watch test/**/*.test.ts --reporter nyan",
        "test:karma": "karma start",
        "lint": "npm run gts:check",
        "serve:dev": "npm run watch",
        "gts:check": "gts check",
        "gts:clean": "gts clean",
        "gts:compile": "tsc -p .",
        "gts:fix": "gts fix",
        "docs": "typedoc src/"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/expect": "^1.20.4",
        "@types/mocha": "^5.2.7",
        "@types/node": "^10.0.3",
        "@types/sinon": "^7.5.0",
        "babel-eslint": "^10.0.3",
        "chai": "^4.2.0",
        "gts": "^1.1.0",
        "karma": "^4.2.0",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "^3.0.0",
        "karma-firefox-launcher": "^1.1.0",
        "karma-mocha": "^1.3.0",
        "karma-sinon": "^1.0.5",
        "karma-typescript-preprocessor": "^0.4.0",
        "mocha": "^6.2.0",
        "mocha-junit-reporter": "^1.23.1",
        "pre-commit": "^1.2.2",
        "rollup": "^1.18.0",
        "rollup-plugin-commonjs": "^10.0.1",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-typescript": "^1.0.1",
        "sinon": "^7.5.0",
        "ts-node": "^8.3.0",
        "typescript": "^3.5.3"
    },
    "license": "Apache-2.0"
}
